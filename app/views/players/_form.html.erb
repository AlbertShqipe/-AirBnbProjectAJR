<%= simple_form_for(player) do |f| %>
  <%= f.input :name %>
  <%= f.input :position %>
  <%= f.input :age %>
  <%= f.input :address %>
  <%= f.input :nationality %>
  <%= f.input :height %>
  <%= f.input :foot %>
  <%= f.input :club %>
  <%= f.input :market_value %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
